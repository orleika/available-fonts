{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-console": 0,
    "no-dupe-class-members": 2,
    "no-empty-pattern": 2,
    "no-this-before-super": 2,
    "prefer-const": 2,
    "prefer-reflect": 2,
    "require-yield": 2
  }
}
